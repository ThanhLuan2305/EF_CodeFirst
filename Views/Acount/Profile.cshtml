@model EF_CodeFirst.Identity.AppUser

@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Profile</h2>

<div>
    <p>@Model.UserName</p>
    <p>@Model.Email</p>
    <p>@Model.Birthday</p>
    <p>@Model.PhoneNumber</p>
    <p>@Model.Addess</p>
    <p>@Model.City</p>
</div>
